import { useParams } from "react-router-dom";

export default function IntentionPage() {
    const { token, init_date } = useParams();
    const startDate = new Date(Number(init_date))
    const endDate = new Date(Number(init_date))
    endDate.setHours(24 * 6 + endDate.getHours())
    

    return (

        <>
            <div className="asd00">
                <div className="asd01">Intencje Mszy Œwiêtych</div>
                <div className="asd02">
                    {startDate.getDate() + '.' + startDate.getMonth() + '.' + startDate.getFullYear() + ' r. - ' + endDate.getDate() + '.' + endDate.getMonth() + '.' + endDate.getFullYear() + ' r.'}
                </div>
            </div >
        </>
    );
}